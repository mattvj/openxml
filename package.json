{
  "name": "openxml",
  "version": "1.0.2",
  "description": "Open XML SDK for JavaScript",
  "scripts": {
    "lint": "eslint performance src --ignore-path .gitignore",
    "test": "jest --config .jestrc"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --single-quote --trailing-comma=all --write",
      "git add"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattvj/openxml.git"
  },
  "keywords": [
    "openxml",
    "microsoft",
    "office",
    "word",
    "powerpoint",
    "excel",
    "docx",
    "xlsx",
    "pptx"
  ],
  "author": "Matt Johnson",
  "license": "Ms-PL",
  "bugs": {
    "url": "https://github.com/mattvj/openxml/issues"
  },
  "homepage": "https://github.com/mattvj/openxml#readme",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "jest": "^20.0.4",
    "jszip": "^3.1.3",
    "linq": "^3.0.8",
    "lint-staged": "^3.4.2",
    "mockdate": "^2.0.1",
    "prettier": "^1.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-recommended": "^1.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-config-prettier": "^2.1.0",
    "eslint-plugin-jest": "^20.0.3"
  },
  "babel": {
    "plugins": [
      "transform-class-properties"
    ]
  }}
